{
  "name": "noname-shop",
  "private": true,
  "version": "0.0.0",
  "author": "Victor Maznichenko",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write '**/*.{ts,tsx,js,json,css,scss,html}'",
    "lint": "eslint . --ext ts,tsx",
