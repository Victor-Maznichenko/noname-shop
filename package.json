    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "path": "^0.12.7",
    "postcss": "^8.4.34",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
