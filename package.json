    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.22.3",
    "swiper": "^11.0.6",
    "tailwindcss": "^3.4.1"
